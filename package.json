{
  "name": "@bimdata/windowmanager",
  "version": "0.1.0",
  "description": "A Vuejs Blender style multi resizable/editable window manager to create custom layouts.",
  "main": "dist/windowmanager.js",
  "module": "dist/windowmanager.esm.js",
  "unpkg": "dist/windowmanager.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "prebuild": "rm -rf dist",
    "start": "rollup --watch -c --environment BUILD:development",
    "prepublishOnly": "npm run build"
  },
  "author": "BIMData.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bimdata/viewer-window-manager.git"
  },
  "bugs": {
    "url": "https://github.com/bimdata/viewer-window-manager/issues"
  },
  "homepage": "https://github.com/bimdata/viewer-window-manager#readme",
  "license": "MIT",
  "keywords": [
    "window",
    "layout",
    "resize",
    "drag and drop"
  ],
  "devDependencies": {
    "rollup": "^2.8.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "5.1.6",
    "vue-template-compiler": "^2.6.11"
  }
}
